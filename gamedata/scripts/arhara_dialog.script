function radar_udalyem_art()
    art_v_buliz_virogdeno_count = 0
    for a,v in pairs(tabl_art_v_buliz) do
        --если вероятность вырождения "сработала"
        if math_random() < v then
            --а сколько у ГГ этих артов то ?
