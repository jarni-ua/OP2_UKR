            db.actor:iterate_inventory(function (dummy, vItem)
                if vItem:section() == a then
                    aCount = aCount + 1 
                end 
            end, nil)
              --есть такие арты
            if aCount >= 1 then 
                sak.out_item_namber(a, 1)
                art_v_buliz_virogdeno_count = art_v_buliz_virogdeno_count + 1
            end
        end
    end
end
